#include "Cworking_Signal_Processing_Application_Layer.h"

void Cworking_Application_Layer::cworking_process_OFDM_modulation( Cworking_Input_Data& cworking_input, Cworking_Output_Data& cworking_output )
{
	for ( int cworking_symbol = 0; cworking_symbol < cworking_input.cworking_radio_frame.rows; cworking_symbol++ )
	{
		/* Create temporary symbol vector */
		Cworking_Complex_Vector cworking_symbol_input;

		/* Create temporary symbol vector */
		Cworking_Complex_Vector cworking_symbol_output;

		/* Get single vector */
		cworking_symbol_input.array  = cworking_input.cworking_radio_frame.matrix[ cworking_symbol ];

		/* Store vector size - FFT size */
		cworking_symbol_input.length = cworking_input.cworking_radio_frame.cols;

		/* Calculate IFFT */
		this->cworking_dsp.muged_1D_ifft( cworking_symbol_input, cworking_symbol_output );

	}
}

void Cworking_Application_Layer::cworking_process_OFDM_demodulation( Cworking_Input_Data& cworking_input, Cworking_Output_Data& cworking_output )
{
}
