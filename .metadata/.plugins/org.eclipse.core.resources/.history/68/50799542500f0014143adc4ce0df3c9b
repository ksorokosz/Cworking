#include "Cworking_Signal_Processing_Application_Layer.h"

void Cworking_Application_Layer::cworking_process_OFDM_modulation( Cworking_Input_Data& cworking_input, Cworking_Output_Data& cworking_output )
{
	for ( int cworking_row = 0; cworking_row < cworking_input.cworking_matrix.rows; cworking_row++ )
	{
		/* Create temporary vector */
		Cworking_Complex_Vector cworking_vector;

		/* Get single vector */
		cworking_vector.array  = cworking_input.cworking_matrix.matrix[ cworking_row ];

		/* Store vector size - FFT size */
		cworking_vector.length = cworking_input.cworking_matrix.cols;
	}
}

void Cworking_Application_Layer::cworking_process_OFDM_demodulation( Cworking_Input_Data& cworking_input, Cworking_Output_Data& cworking_output )
{
}
