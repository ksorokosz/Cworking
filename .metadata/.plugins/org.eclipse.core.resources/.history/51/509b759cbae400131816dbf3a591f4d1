#include "CW_Memory_Allocator_Main.h"

int main()
{
	/* Declare simple memory pool */
	CW_Memory_Pool cw_memory_pool;

	/* Memory block */
	CW_uint32_t  cw_block = 0;
	CW_uint32_t* array    = NULL;

	/* Create memory pool */
	CREATE_MEMORY_POOL(CW_uint32_t, 10000, &cw_memory_pool );

	/* For each memory block */
	for ( ; cw_block < 10000; cw_block++ )
	{
		MALLOC_STATIC( );
	}

	for ( cw_block = 0; cw_block < 10000; cw_block++ )
	{
		FREE_STATIC(&cw_memory_pool, )
	}

	/* Remove memory pool */
	REMOVE_MEMORY_POOL( &cw_memory_pool );

	return 0;
}
